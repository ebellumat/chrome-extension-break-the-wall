{
  "name": "Break the wall",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "This extension allows you to read any website without distractions or ads of any type",
  "content_scripts": [ {
      "js": [ "content_script.js"  ],
      "matches": [ "*://*/*"]    
  } ],
  "permissions" : [
      "activeTab"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" }
}